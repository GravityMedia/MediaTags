{
    "name": "gravitymedia/metadata",
    "description": "Metadata library for PHP",
    "keywords": [
        "gravitymedia",
        "metadata"
    ],
    "homepage": "https://github.com/GravityMedia/Metadata",
    "license": "MIT",
    "authors": [
        {
            "name": "Daniel SchrÃ¶der",
            "email": "daniel.schroeder@gravitymedia.de",
            "homepage": "https://github.com/pCoLaSD",
            "role": "Developer"
        }
    ],
    "require": {
        "php": ">=5.6",
        "gravitymedia/stream": "^1.0@dev",
        "mdurrant/php-binary-reader": "^1.0"
    },
    "require-dev": {
        "debach/zend-mp3": "@dev",
        "james-heinrich/getid3": "^1.9",
        "phpunit/phpunit": "^5.2",
        "scrutinizer/ocular": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "GravityMedia\\Metadata\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GravityMedia\\MetadataTest\\": "tests/src/"
        }
    },
    "scripts": {
        "test": "php vendor/bin/phpunit"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    },
    "archive": {
        "exclude": [
            "/.*",
            "/composer.*",
            "/phpunit.*",
            "/resources",
            "/tests"
        ]
    }
}
